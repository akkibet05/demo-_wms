    function ValidationForMaxQTY(i){
        var prod_qty = document.getElementById("prod_qty"+i).value;
        var stocks_qty = document.getElementById("MaxStocks_data"+i).value;
        if(parseInt(prod_qty) >  parseInt(stocks_qty)){
            Swal.fire(
            'Sorry',
